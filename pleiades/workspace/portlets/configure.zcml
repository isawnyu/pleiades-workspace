<configure
  xmlns="http://namespaces.zope.org/zope"
  xmlns:plone="http://namespaces.plone.org/plone"
  i18n_domain="pleiades.workspace"
  >

  <include package="plone.app.portlets" />

  <plone:portlet
    name="pleiades.Workspaces"
    interface=".workspaces.IWorkspacesPortlet"
    assignment=".workspaces.Assignment"
    renderer=".workspaces.Renderer"
    addview=".workspaces.AddForm"
    editview=".workspaces.EditForm"
    />
    
</configure>
